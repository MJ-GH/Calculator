<Page xmlns="http://schemas.nativescript.org/tns.xsd" navigatingTo="navigatingTo" backgroundColor="grey">
    <ActionBar title="Calculator - Select Number" icon="" />

    <StackLayout verticalAlignment="center" horizontalAlignment="center">
        <TextField hint="Type in number here" text="{{selectedNumber}}" class="h2"/>
        <button text="Ok" backgroundColor="#289062" tap="{{sendNumberToMain}}" />
    </StackLayout>
</Page>
